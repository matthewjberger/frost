Position :: struct {
    x: f64,
    y: f64,
}

mut positions := [
    Position { x = 0.0, y = 0.0 },
    Position { x = 1.0, y = 1.0 },
    Position { x = 2.0, y = 2.0 },
]

print("Before:")
for i in 0..3 {
    pos := positions[i]
    print(pos.x)
}

positions[0] = Position { x = 100.0, y = 100.0 }
positions[1] = Position { x = 200.0, y = 200.0 }

print("After:")
for i in 0..3 {
    pos := positions[i]
    print(pos.x)
}
